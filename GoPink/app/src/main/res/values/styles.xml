<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <color name="textViewColor">#F06292</color>
    <color name="textColor">#FFFFFF</color>
    <color name="actionBarColor">#E91E63</color>
    <color name="statusBarColor">#C2185B</color>
    <!-- ActionBar styles -->

    <style name="AppTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here.-->
       <item name="colorPrimary">@color/actionBarColor</item>
        <item name="colorAccent">@color/textColor</item>
        <item name="colorPrimaryDark">@color/statusBarColor</item>
        <item name="android:navigationBarColor" tools:targetApi="lollipop">@color/statusBarColor</item>
    </style>
</resources>
